{
	"dependencies": {
		"glob": "12.0.0",
		"ts-mixer": "6.0.4",
		"ts-pattern": "5.9.0",
		"zod": "4.1.12"
	},
	"description": "An opinionated framework made in TypeScript for building Discord bots.",
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@commitlint/cli": "20.1.0",
		"@commitlint/config-conventional": "20.0.0",
		"@commitlint/types": "20.0.0",
		"@swc/core": "1.15.1",
		"@types/node": "24.10.0",
		"esbuild-plugin-version-injector": "1.2.1",
		"husky": "9.1.7",
		"lint-staged": "16.2.6",
		"rimraf": "6.1.0",
		"tsup": "8.5.0",
		"typescript": "5.9.3",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "4.0.8"
	},
	"engines": {
		"node": ">=22.18.0"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs"
		}
	},
	"files": [
		"LICENSE",
		"README.md",
		"dist"
	],
	"license": "MIT",
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,json}": [
			"pnpm biome:write"
		]
	},
	"main": "dist/index.mjs",
	"module": "dist/index.mjs",
	"name": "linkcord",
	"optionalDependencies": {
		"@discordjs/voice": "0.19.0"
	},
	"packageManager": "pnpm@10.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/FancyStudioTeam/Linkcord"
	},
	"scripts": {
		"biome": "biome check",
		"biome:ci": "biome ci",
		"biome:write": "biome check --write",
		"build": "tsup",
		"prebuild": "pnpm rimraf dist",
		"prepare": "husky",
		"pretarball": "pnpm build",
		"release": "pnpm publish --access public --no-git-checks",
		"rimraf": "rimraf dist",
		"tarball": "pnpm pack",
		"vitest": "vitest --typecheck -c vitest.config.ts",
		"vitest:watch": "vitest --typecheck -c vitest.config.ts --watch"
	},
	"type": "module",
	"types": "dist/index.d.ts",
	"version": "0.0.0"
}
