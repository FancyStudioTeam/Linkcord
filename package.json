{
	"dependencies": {
		"glob": "11.0.3",
		"ts-mixer": "6.0.4",
		"valibot": "1.1.0"
	},
	"description": "An opinionated framework made in TypeScript for building Discord bots.",
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@commitlint/cli": "19.8.1",
		"@commitlint/config-conventional": "19.8.1",
		"@commitlint/types": "19.8.1",
		"@microsoft/api-extractor": "7.52.13",
		"@swc/core": "1.13.5",
		"@types/node": "24.5.2",
		"esbuild-plugin-version-injector": "1.2.1",
		"husky": "9.1.7",
		"lint-staged": "16.2.0",
		"rimraf": "6.0.1",
		"tsup": "8.5.0",
		"typedoc": "0.28.13",
		"typedoc-material-theme": "1.4.0",
		"typescript": "5.9.2",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.4"
	},
	"engines": {
		"node": ">=22.18.0"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./jsx-runtime": {
			"import": "./dist/jsx/jsx-runtime.mjs",
			"require": "./dist/jsx/jsx-runtime.js"
		}
	},
	"files": [
		"LICENSE",
		"README.md",
		"dist"
	],
	"license": "MIT",
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,json}": [
			"pnpm biome:write"
		]
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"name": "linkcord",
	"optionalDependencies": {
		"@discordjs/voice": "0.19.0"
	},
	"packageManager": "pnpm@10.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/FancyStudioTeam/Linkcord"
	},
	"scripts": {
		"biome": "biome check",
		"biome:ci": "biome ci",
		"biome:write": "biome check --write",
		"build": "tsup",
		"bump": "tsx scripts/bump.ts",
		"check-release-tags": "tsx scripts/checkReleaseTags.ts",
		"prebuild": "pnpm rimraf dist",
		"prepare": "husky",
		"pretarball": "pnpm build",
		"release": "pnpm publish --access public --no-git-checks",
		"rimraf": "rimraf dist",
		"tarball": "pnpm pack",
		"typedoc": "typedoc",
		"vitest": "vitest --typecheck -c vitest.config.ts",
		"vitest:watch": "vitest --typecheck -c vitest.config.ts --watch"
	},
	"types": "dist/index.d.ts",
	"version": "0.0.0"
}
