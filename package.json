{
	"dependencies": {
		"zod": "4.1.13"
	},
	"description": "An opinionated framework made in TypeScript for building Discord bots.",
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@commitlint/cli": "20.2.0",
		"@commitlint/config-conventional": "20.2.0",
		"@commitlint/types": "20.2.0",
		"@microsoft/api-extractor": "7.55.2",
		"@swc/core": "1.15.3",
		"@types/node": "25.0.1",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"rimraf": "6.1.2",
		"tsup": "8.5.1",
		"tsx": "4.21.0",
		"typescript": "5.9.3",
		"unplugin-replace": "0.6.2",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "4.0.15"
	},
	"engines": {
		"node": ">=22.18.0"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"LICENSE",
		"README.md",
		"dist"
	],
	"license": "MIT",
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,json}": [
			"pnpm biome:write"
		]
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"name": "linkcord",
	"optionalDependencies": {
		"@discordjs/voice": "0.19.0"
	},
	"packageManager": "pnpm@10.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/FancyStudioTeam/Linkcord"
	},
	"scripts": {
		"biome:check": "biome check",
		"biome:ci": "biome ci",
		"biome:write": "biome check --write",
		"build": "tsup",
		"extractor:check-release-tags": "tsx scripts/checkReleaseTags.ts",
		"prebuild": "pnpm rimraf dist",
		"prepare": "husky",
		"pretarball": "pnpm build",
		"release": "pnpm publish --access public --no-git-checks",
		"rimraf": "rimraf dist",
		"tarball": "pnpm pack",
		"vitest:run": "vitest --typecheck -c vitest.config.ts",
		"vitest:watch": "vitest --typecheck --watch -c vitest.config.ts"
	},
	"type": "module",
	"types": "dist/index.d.ts",
	"version": "0.0.0"
}
