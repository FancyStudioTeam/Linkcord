{
  "dependencies": {
    "@fancystudioteam/linkcord": "link:packages/linkcord"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@types/semver": "7.7.0",
    "husky": "9.1.7",
    "lint-staged": "16.0.0",
    "semver": "7.7.1",
    "turbo": "2.5.3",
    "typescript": "5.8.3"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json}": ["pnpm biome:check"]
  },
  "name": "linkcord-monorepo",
  "packageManager": "pnpm@10.10.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/FancyStudioTeam/Linkcord"
  },
  "scripts": {
    "biome:check": "biome check --write",
    "build": "turbo run build",
    "bump": "node scripts/bump.mjs",
    "extractor:run": "turbo run extractor:run",
    "prepare": "husky",
    "release": "turbo run release",
    "tarball": "turbo run tarball",
    "vitest": "turbo run vitest",
    "vitest:watch": "turbo run vitest:watch"
  }
}
