{
	"author": "Linkcord Team",
	"dependencies": {
		"zod": "4.3.5"
	},
	"description": "An opinionated framework for building Discord bots using TypeScript",
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@commitlint/cli": "20.3.0",
		"@commitlint/config-conventional": "20.2.0",
		"@commitlint/types": "20.2.0",
		"@microsoft/api-extractor": "7.55.2",
		"@types/node": "25.0.3",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"rimraf": "6.1.2",
		"tsc-alias": "1.8.16",
		"tsx": "4.21.0",
		"typescript": "5.9.3",
		"vite-tsconfig-paths": "6.0.3",
		"vitest": "4.0.16"
	},
	"engines": {
		"node": ">=22.18.0"
	},
	"exports": {
		".": {
			"import": "./lib/index.js",
			"require": "./lib/index.js"
		},
		"./builders": {
			"import": "./lib/builders/index.js",
			"require": "./lib/builders/index.js"
		},
		"./handlers": {
			"import": "./lib/handlers/index.js",
			"require": "./lib/handlers/index.js"
		},
		"./types": {
			"import": "./lib/types/index.js",
			"require": "./lib/types/index.js"
		},
		"./utils": {
			"import": "./lib/utils/index.js",
			"require": "./lib/utils/index.js"
		}
	},
	"files": [
		"lib",
		"LICENSE",
		"README.md"
	],
	"keywords": [
		"discord",
		"framework",
		"linkcord",
		"typescript"
	],
	"license": "MIT",
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,json}": [
			"pnpm biome:write"
		]
	},
	"main": "lib/index.js",
	"module": "lib/index.js",
	"name": "linkcord",
	"optionalDependencies": {
		"@discordjs/voice": "0.19.0"
	},
	"packageManager": "pnpm@10.0.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/FancyStudioTeam/Linkcord"
	},
	"scripts": {
		"biome:check": "biome check",
		"biome:ci": "biome ci",
		"biome:write": "biome check --write",
		"build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"extractor:check-release-tags": "tsx scripts/checkReleaseTags.ts",
		"prebuild": "pnpm rimraf",
		"prepare": "husky",
		"pretarball": "pnpm build",
		"release": "pnpm publish --access public --no-git-checks",
		"rimraf": "rimraf lib",
		"tarball": "pnpm pack",
		"vitest:run": "vitest --typecheck -c vitest.config.ts",
		"vitest:watch": "vitest --typecheck --watch -c vitest.config.ts"
	},
	"type": "module",
	"types": "lib/index.d.ts",
	"version": "0.0.0"
}
