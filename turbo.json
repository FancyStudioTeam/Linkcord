{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "extractor:run": {
      "dependsOn": ["build"]
    },
    "release": {
      "dependsOn": ["build"]
    },
    "tarball": {
      "dependsOn": ["build"]
    },
    "vitest": {},
    "vitest:watch": {}
  }
}
